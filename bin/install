#!/bin/bash
#set -e

version=2.1.1

cp -r ${OPENSHIFT_PLAYFRAMEWORK_DIR}/versions/${version}/template/* ${OPENSHIFT_PLAYFRAMEWORK_DIR}/template
cp -r ${OPENSHIFT_PLAYFRAMEWORK_DIR}/versions/${version}/bin/* $OPENSHIFT_PLAYFRAMEWORK_DIR/bin
cp -r ${OPENSHIFT_PLAYFRAMEWORK_DIR}/versions/${version}/play-${version}/* $OPENSHIFT_PLAYFRAMEWORK_DIR/play-${version}
